{
  "name": "@szymmis/scaf",
  "description": "A simple tool for scaffolding Advent of Code tasks",
  "version": "0.9.0",
  "author": "szymmis",
  "repository": "https://github.com/szymmis/scaf",
  "private": false,
  "license": "MIT",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "scaf": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun --watch src/bin/index.ts",
    "build": "bun scripts/build.ts",
    "test": "bun vitest run src",
    "pack": "bun run test && bun run build && npm pack"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "commander": "^12.1.0",
    "open": "^9.1.0",
    "smol-toml": "^1.3.1",
    "vitest": "^2.1.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "keywords": [
    "advent of code",
    "advent",
    "of",
    "code",
    "aoc",
    "scaffolding",
    "cli"
  ]
}
